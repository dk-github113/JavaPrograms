class Main
{
    public static void main(String s[])
    {
        int n=34543,r,sum=0,temp;
        System.out.println(n);
        temp=n;
        while(n>0)
        {
            r=n%10;
            sum=r+(sum*10);
            n=n/10;
        }
        if(temp==sum)
            System.out.println("it is palindrom number");
            else
            System.out.println("not palindrom");
        
        
       
        }
    }
