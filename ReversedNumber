class Main
{
    public static void main(String s[])
    {
        int n=12345,r,rev=0;
        System.out.println(n);
        while(n>0)
        {
            r=n%10;
            rev=rev*10+r;
            n=n/10;
        }
        System.out.println("reversed number"+rev);
        
       
        }
    }
