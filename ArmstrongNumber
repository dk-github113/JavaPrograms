class Main
{
    public static void main(String s[])
    {
        int n=371,r,arm=0,temp;
        System.out.println(n);
        temp=n;
        while(n>0)
        {
            r=n%10;
            arm=arm+(r*r*r);
            n=n/10;
        }
        if(temp==arm)
            System.out.println("it is armstrong number");
            else
            System.out.println("not armstrong number");
        }
    }
